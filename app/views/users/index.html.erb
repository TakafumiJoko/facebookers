<div class="col-xs-3">
<%= render 'shared/side_user', user: @user %>
<%= render 'shared/side_book' %>
</div>
  <div class="col-xs-9">
    <h2>Users</h2>
    <table class="table">
      <tr>
        <th>image</th>
        <th>name</th>
        <th colspan="3"></th>
      </tr>
      <% @users.each do |user| %>
      <tr>
        <td><%= attachment_image_tag user, :profile_image, size:'80x80', fallback: "no_image.jpg" %></td>
        <td><%= user.name %></td>
        <td><%= link_to "Show", user_path(user.id) %></td>
      </tr>
      <% end %>
    </table>
  </div>
